{
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "rescript-react-intl",
    "rescript-react-update",
    "rescript-webapi"
  ],
  "bsc-flags": ["-open RescriptCore"],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "Js": "Js",
      "Map": "Map"
    },
    "generatedFileExtension": ".gen.tsx",
    "module": "esmodule",
    "moduleResolution": "node",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "name": "check.beer",
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js"
}
