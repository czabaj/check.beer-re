{
  "bs-dependencies": [
    "@nobleai/rescript-rxjs",
    "@rescript/core",
    "@rescript/react",
    "@rescriptbr/reform",
    "@rescriptbr/reschema",
    "rescript-react-intl",
    "rescript-react-update",
    "rescript-webapi"
  ],
  "bs-dev-dependencies": ["rescript-vitest"],
  "bsc-flags": ["-open RescriptCore"],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "generatedFileExtension": ".gen.tsx",
    "module": "es6",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "name": "check.beer",
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "ppx-flags": ["lenses-ppx/ppx"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "type": "dev",
      "subdirs": true
    }
  ],
  "suffix": ".res.js"
}
